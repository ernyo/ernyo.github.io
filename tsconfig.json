{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext", //"commonjs",
    "moduleResolution": "node",
    // "strict": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {},
